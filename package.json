{
  "name": "vue-3d-loader",
  "version": "1.3.6",
  "private": false,
  "main": "dist/vue-3d-loader.umd.min.js",
  "unpkg": "dist/vue-3d-loader.min.js",
  "jsdelivr": "dist/vue-3d-loader.min.js",
  "description": "VueJS and threeJS 3d viewer",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:lib && npm run build:wc",
    "build:lib": "vue-cli-service build --target lib ./src/index.js --name vue-3d-loader --dest dist",
    "build:wc": "vue-cli-service build --target wc --no-clean ./src/3dLoader/vue3dLoader.vue --name vue-3d-loader --dest dist",
    "build:example": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "directories": {
    "dist": "dist"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "three": "^0.142.0",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "vue-template-compiler": "^2.6.14"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/king2088/vue-3d-loader.git"
  },
  "bugs": {
    "url": "https://github.com/king2088/vue-3d-loader/issues"
  },
  "homepage": "https://github.com/king2088/vue-3d-loader",
  "keywords": [
    "vue",
    "3d",
    "threeJS",
    "3d viewer",
    "vue 3d"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "license": "MIT"
}
